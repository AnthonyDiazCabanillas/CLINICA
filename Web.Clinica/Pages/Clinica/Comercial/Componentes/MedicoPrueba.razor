@using Ent.Sql.ClinicaE.MedicosE
@using Ent.Sql.ClinicaE.EspecialidadesE
@using Web.Clinica.Components
@using Ent.Sql.ClinicaE.OtrosE



<div style="background-color: #FFF; width: 100%; height:400px; border:1px solid #b3d6ffff; border-radius:8px; padding:20px 10px; margin-bottom:20px;">
    <div class="row">

        <div class="col-sm-12 col-md-6  col-lg-4">

            <Article Title="A new hope" DatePublished="@DateTime.Now">
                <Summary>
                    This article was posted on @context.DatePublished.ToShortDateString()
                    and talks about @context.Title.
                    DR (@context.derecho_reservado)
                </Summary>
                <Body>
                    <p>This is the post content</p>
                    <p>Today is @DateTime.Now</p>
                    <p>Blah, blah, blah...</p>
                </Body>
            </Article>

        </div>




        <div class="col-sm-12 col-md-6  col-lg-4">
            <!--I.DocIdentidad-->
            <div class="omrs-input-group">
                <label class="omrs-input-underlined">
                    <input id="txtDocIdentidadReg" required="" @bind-value="@oMedicoE.DocIdentidad" disabled>
                    <span class="omrs-input-label">Doc.Identidad</span>
                    <span class="omrs-input-helper"></span>
                </label>
            </div>


            <!--F.DocIdentidad-->

        </div>



        <div class="col-sm-12 col-md-6  col-lg-4">
        </div>

    </div>

</div>




@code {
    [Parameter]
    public MedicoE oMedicoE { get; set; }

    [Parameter]
    public List<EspecialidadxMedicoE> oListaEspecialidades { get; set; }

    [Parameter]
    public List<TablasE> oListaTablasAFP { get; set; }

    [Parameter]
    public List<TablasE> oListaTablasTipDocEmitira { get; set; }

    protected override void OnInitialized()
    {

    }



 }
